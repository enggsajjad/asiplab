#!/usr/bin/perl
# This script generates wire_inout instance in behavior level
# parameter : instance_name bit_width

if ($#ARGV != 2) {
  print "number of parameters is wrong.\n";
  print "usage : this_script name_space instance_name bit_width\n";
  exit (100);
}

$name_space    = $ARGV[0];
$instance_name = $ARGV[1];
$bit_width     = $ARGV[2];

#if ($bit_width == 1){
#    $port_type = "bool";
#}
if ( $bit_width <= 64 ) {
    $port_type = "sc_dt::sc_uint<${bit_width}>";
}
else{
    $port_type = "sc_dt::sc_biguint<${bit_width}>";
}

$hiz = "Z" x $bit_width;

{
    print <<FHM_DL_INSTANCE;
#include "${instance_name}.h"

// Module     : $bit_width-bit input/output wire
// Feature    : 
// References : refer to wire.fhm by Y.Kobayashi (c)2003.
// Author     : Designed by K.Ueda (c)2004.
// Version    : 1.0 :
// Functionality :
//  port
//   dir_read     : direction selection.
//                  '1' for in  (read)
//                  '0' for out (write)
//   int_in_port  : internal input port
//   int_out_port : internal output port
//   ext_port     : external port
// Comment :

void ${name_space}::${instance_name}::process()
{
  if ( dir_read.read() == 0 )
    ext_port.write( int_in_port.read() );
  else {
    ext_port.write( "$hiz" );
    int_out_port.write( $port_type(ext_port.read()) );
  }
}
FHM_DL_INSTANCE
}

exit(0);
          ]]>
        </script>

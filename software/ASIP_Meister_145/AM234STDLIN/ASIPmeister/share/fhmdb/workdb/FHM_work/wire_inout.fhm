#!/usr/bin/perl
# This script generates wire_inout function description in synthesis level
# parameter : bit_width

if ($#ARGV != 0) {
    print "number of parameters is wrong.\n";
    print "usage : this_script bit_width\n";
    exit (100);
}

$bit_width    = $ARGV[0];

if ($bit_width > 80) {
    if ($bit_width != 128) {
    print "bit_width $bit_width is not supported.\n";
    exit (100);
    }
}
elsif ($bit_width < 1) {
    print "bit_width $bit_width is not supported.\n";
    exit (100);
}

if ("$bit_width" == "1") {
    $range = "";
    $vac   = "";
}
else {
    $W1 = $bit_width - 1;
    $range = "[$W1:0]";
    $vec   = "_vector";
}

{
    print <<FHM_DL_FUNC_DESC
/** $bit_width-bit input/output wire */
model wire_inout_${bit_width}{
  port{
    in    dir_read:
    in    int_in_port$range;
    out   int_out_port$range;
    inout ext_port$range;
  }

  /** no operation */
  function nop : idle{
    control {
      in dir_read;
    }
    protocol {
      [dir_read == '1']{
        int_out_port = ext_port;
      }
    }
  }

  /** write */
  function write{
    input {
      bit$vec int_in_port;
    }
    output {
    }
    control {
      in dir_read;
    }
    protocol {
      [dir_read == '0']{
        ext_port = int_in_port;
      }
    }
  }

  /** read */
  function read {
    input {
    }
    output {
      bit$vec int_out_port;
    }
    control {
      in dir_read;
    }
    protocol {
      [dir_read == '1']{
        int_out_port = ext_port;
      }
    }
  }
}
FHM_DL_FUNC_DESC
}
exit (0);
        ]]>
      </script>
    </function_description>


          <![CDATA[
#!/usr/bin/perl
# This script generates wire_inout instance in behavior level
# parameter : instance_name bit_width

if ($#ARGV != 1) {
  print "number of parameters is wrong.\n";
  print "usage : this_script instance_name bit_width\n";
  exit (100);
}

$instance_name = $ARGV[0];
$bit_width     = $ARGV[1];

if ($bit_width == 1){
    $port_type = "std_logic";
    $zs = "'Z'";
}
else{
    $b_1 = $bit_width - 1;
    $port_type = "std_logic_vector($b_1 downto 0)";
    $zs = "\"" . "Z" x $bit_width . "\"";
}

{
    print <<FHM_DL_INSTANCE;
-- Module     : $bit_width-bit input/output wire
-- Feature    : 
-- References : refer to wire.fhm by Y.Kobayashi (c)2003.
-- Author     : Designed by K.Ueda (c)2004.
-- Version    : 1.0 :
-- Functionality :
--  port
--   dir_read     : direction selection.
--                  '1' for in  (read)
--                  '0' for out (write)
--   int_in_port  : internal input port
--   int_out_port : internal output port
--   ext_port     : external port
-- Comment :

library IEEE;
use IEEE.std_logic_1164.all;

entity $instance_name is
  port (
    dir_read     : in    std_logic;
    int_in_port  : in    $port_type;
    int_out_port : out   $port_type;
    ext_port     : inout $port_type);
end $instance_name;

architecture behavior of $instance_name is
begin
  process (dir_read, int_in_port, ext_port)
  begin
    if (dir_read = '0') then
      ext_port <= int_in_port;
    else
      ext_port <= $zs;
      int_out_port <= ext_port;
    end if;
  end process;
end behavior;
FHM_DL_INSTANCE
}

exit (0);
          ]]>
        </script>
      </instance>


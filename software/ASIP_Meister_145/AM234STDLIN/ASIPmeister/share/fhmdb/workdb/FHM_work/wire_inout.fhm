#!/usr/bin/perl
# This script generates wire_inout instance in behavior level
# parameter : instance_name bit_width

if ($#ARGV != 1) {
  print "number of parameters is wrong.\n";
  print "usage : this_script instance_name bit_width\n";
  exit (100);
}

$instance_name = $ARGV[0];
$bit_width     = $ARGV[1];

if ($bit_width == 1){
    $port_type = " ";
    $zs = "1'bZ";
}
else{
    $b_1 = $bit_width - 1;
    $port_type = "[${b_1}:0]";
    $zs = "\{${bit_width}\{1'bZ\}\}";
}

{
    print <<FHM_DL_INSTANCE;
// Module     : ${bit_width}-bit input/output wire
// Feature    : 
// References : refer to wire.fhm by Y.Kobayashi (c)2003.
// Author     : Designed by K.Ueda (c)2004.
// Version    : 1.0 :
// Functionality :
//  port
//   dir_read     : direction selection.
//                  '1' for in  (read)
//                  '0' for out (write)
//   int_in_port  : internal input port
//   int_out_port : internal output port
//   ext_port     : external port
// Comment :
\n
module $instance_name (
  dir_read     ,
  int_in_port  ,
  int_out_port ,
  ext_port     );
\n
input         dir_read     ;
input  $port_type int_in_port  ;
output $port_type int_out_port ;
inout  $port_type ext_port     ;
\n
wire         dir_read     ;
wire  $port_type int_in_port  ;
wire  $port_type int_out_port ;
wire  $port_type ext_port     ;
\n
assign  int_out_port = (dir_read == 1'b1) ? ext_port : int_in_port ;
assign  ext_port     = (dir_read == 1'b0) ? int_in_port : $zs ;
\n
endmodule
FHM_DL_INSTANCE
}

exit (0);
          ]]>
        </script>

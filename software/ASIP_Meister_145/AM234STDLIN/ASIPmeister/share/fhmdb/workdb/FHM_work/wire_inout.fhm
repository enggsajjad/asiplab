#!/usr/bin/perl
# This script generates wire_inout instance in behavior level
# parameter : name_space instance_name bit_width

if ($#ARGV != 2) {
  print "number of parameters is wrong.\n";
  print "usage : this_script name_space instance_name bit_width\n";
  exit (100);
}

$name_space     = $ARGV[0];
$instance_name  = $ARGV[1];
$bit_width      = $ARGV[2];

if ($bit_width == 1){
    $port_type = "bool";
}
elsif ( $bit_width <= 64 ) {
    $port_type = "sc_dt::sc_uint<${bit_width}>";
}
else{
    $port_type = "sc_dt::sc_biguint<${bit_width}>";
}

$macro = "__" . uc($name_space) . "_" . uc($instance_name) . "_H__";

{
    print <<FHM_DL_INSTANCE;
#ifndef $macro
#define $macro 1

// Module     : $bit_width-bit input/output wire
// Feature    : 
// References : refer to wire.fhm by Y.Kobayashi (c)2003.
// Author     : Designed by K.Ueda (c)2004.
// Version    : 1.0 :
// Functionality :
//  port
//   dir_read     : direction selection.
//                  '1' for in  (read)
//                  '0' for out (write)
//   int_in_port  : internal input port
//   int_out_port : internal output port
//   ext_port     : external port
// Comment :
FHM_DL_INSTANCE
}

{
    print <<FHM_DL_INSTANCE

#include <systemc>

namespace ${name_space} {
  SC_MODULE(${instance_name}) {
FHM_DL_INSTANCE
}

{
    print <<FHM_DL_INSTANCE
    sc_core::sc_in< bool > dir_read;
    sc_core::sc_in< $port_type > int_in_port;
    sc_core::sc_out< $port_type > int_out_port;
    sc_core::sc_inout< sc_dt::sc_lv<$bit_width> > ext_port;
FHM_DL_INSTANCE
}

{
    print <<FHM_DL_INSTANCE
  
    SC_CTOR(${instance_name}) {
      SC_METHOD(process);
      sensitive << dir_read << int_in_port << ext_port;
    }

    void process();
  };
};

#endif // $macro
FHM_DL_INSTANCE
}

exit (0);
          ]]>
        </script>

function reset{
  input{
  }

  output{
  }

  control{
    in bit reset;
  }

  protocol{
    single_cycle_protocol{
      reset = '1';
    }
  }
}

FHM_DL_NOP_RESET
}
for ($i=0; $i<=$n_write-1; $i++){
    print <<FHM_DL_WRITE
/** write$i */
function write${i}{
  input{
    bit [$n_sel_1:0] w_sel$i;
    bit [$w_1:0] data_in$i;
  }

  output{
  }

  control{
    in bit w_enb$i;
  }

  protocol{
    single_cycle_protocol{
      w_enb$i = '1';
    }
  }
}

FHM_DL_WRITE
}

for ($i=0; $i<=$n_read-1; $i++){
    print <<FHM_DL_READ

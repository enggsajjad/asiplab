sub log2u{
    $num = $_[0];

    for ($i=1, $N=2; $N<$num; $i++, $N*=2){};
    return($i);
}

# ---------------------------------

sub to_comp{
  @buf = ();
  $num = $_[0];
  $width = $_[1];

  for ($x=1; $x<$width; $x++){
    $buf[$x] = $num % 2;
    $num = ($num - $buf[$x]) / 2;
  }
  $buf[$x] = $num % 2;
  return (join("", reverse(@buf)));
}

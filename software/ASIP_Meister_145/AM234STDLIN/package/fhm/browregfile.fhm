void ${name_space}::flag_reg${flag_size}::process()
{
  if ( async_reset.read() == true ) {
      data_out.write($zeros_flag);
  } else if ( clock.event() && clock.read() == true ) {
    if ( reset.read() == true ) {
      data_out.write( 0 );
    } else if ( enb.read() == true ) {
      data_out.write( data_in.read() );
    }
  }
}


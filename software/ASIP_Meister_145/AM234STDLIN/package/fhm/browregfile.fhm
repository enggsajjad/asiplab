#!/usr/bin/perl
# this script generates instance for registerfile in synthesis level
# parameter : instance_name bit_width num_register num_read_port num_write_port

if ($#ARGV != 5){
    print "number of parameters is wrong.\n";
    print "usage : this_script instance_name bit_width num_register num_read_port num_write_port num_bank\n";
    exit (100);
}

$instance_name = $ARGV[0];
$bit_width     = $ARGV[1];
$n_reg         = $ARGV[2];
$n_read        = $ARGV[3];
$n_write       = $ARGV[4];
$n_bank        = $ARGV[5];


@bit_widths = (1,4,8,16,20,24,28,32,36,40,44,48,52,56,60,64,128);
if (! grep($_ eq $bit_width, @bit_widths)){
    print "bit_width $bit_width is not supported.\n";
    exit (100);
}

@n_regs = (4,8,16,32,64,128);
if (! grep($_ eq $n_reg, @n_regs)){
    print "num_register $n_reg is not supported.\n";
    exit (100);
}
@n_reads = (1,2,4,8,16,32,64,128);
if (! grep($_ eq $n_read, @n_reads)){
    print "num_read $n_read is not supported.\n";
    exit (100);
}
@n_writes = (1,2,4,8,16,32,64,128);
if (! grep($_ eq $n_write, @n_writes)){
    print "num_write $n_write is not supported.\n";
    exit (100);
}
@n_banks = (1,2,4);
if(! grep($_ eq $n_bank, @n_banks)){
    print "num_bank $n_bank is not supported.\n";
    exit (100);
}
if ($n_write > $n_reg){
   print "num_register should be equal or grater than num_write_port\n";
   exit(100);
}

$w_1 = $bit_width - 1;
$n_sel = &log2u($n_reg);
$n_sel_1 = $n_sel - 1;
$log_n_bank = &log2u($n_bank);
if ($bit_width == 1){
   $zeors = "'0'";
}
else{
    $zeros = "\"" . 0 x $bit_width . "\"";
}

$flag_size    = 5;
$flag_size_1  = $flag_size - 1;
$streg_size   = $bit_width - $flag_size;
$streg_size_1 = $streg_size - 1;

$zeros_flag      = "0" x $flag_size;
$zeros_flag      = "\"". $zeros_flag . "\"";
$zeros_streg     = "0" x $streg_size;
$zeros_streg     = "\"" . $zeros_streg . "\"";

{
print <<FHM_DL_TOP
-- Module     : $flag_size-bit positive edge trigger register
-- References :
-- Author     : Designed by Hirofumi IWATO (c) 2008.
-- Version    : 1.0  :

-- Functionality : synthesis level
--  port
--   clock       : clock
--   async_reset : asynchronous reset
--   reset       : synchronous reset
--   enb         : '1' then register data
--   data_in     : data to register
--   data_out    : registered data

library IEEE;
  use IEEE.std_logic_1164.all;
  use IEEE.std_logic_unsigned.all;

entity flag_reg$flag_size is
  port (clock       : in  std_logic;
        async_reset : in  std_logic;
        reset       : in  std_logic;
        enb         : in  std_logic;
        data_in     : in  std_logic_vector($flag_size_1 downto 0);
        data_out    : out std_logic_vector($flag_size_1 downto 0) );
end flag_reg$flag_size;

architecture synthesis of flag_reg$flag_size is
begin

  process (clock, async_reset)
  begin
    if (async_reset = '1') then
      data_out <= $zeros_flag;
    elsif (clock'event and clock = '1') then
      if (reset = '1') then
        data_out <= (others => '0');
      elsif (enb = '1') then
        data_out <= data_in;
      end if;
    end if;
  end process;

end synthesis;

--%%'

-- Module     : $streg_size-bit positive edge trigger register
-- References :
-- Author     : Designed by Hirofumi IWATO (c) 2008.
-- Version    : 1.0  :

-- Functionality : synthesis level
--  port
--   clock       : clock
--   async_reset : asynchronous reset
--   reset       : synchronous reset
--   enb         : '1' then register data
--   data_in     : data to register
--   data_out    : registered data

library IEEE;
  use IEEE.std_logic_1164.all;
  use IEEE.std_logic_unsigned.all;

entity status_reg$streg_size is
  port (clock       : in  std_logic;
        async_reset : in  std_logic;
        reset       : in  std_logic;
        enb         : in  std_logic;
        data_in     : in  std_logic_vector($streg_size_1 downto 0);
        data_out    : out std_logic_vector($streg_size_1 downto 0) );
end status_reg$streg_size;

architecture synthesis of status_reg$streg_size is
begin

  process (clock, async_reset)
  begin
    if (async_reset = '1') then
      data_out <= $zeros_streg;
    elsif (clock'event and clock = '1') then
      if (reset = '1') then
        data_out <= (others => '0');
      elsif (enb = '1') then
        data_out <= data_in;
      end if;
    end if;
  end process;


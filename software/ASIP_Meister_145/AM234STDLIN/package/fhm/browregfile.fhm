    in bit flag_enb;
  }

  protocol{
    single_cycle_protocol{
      flag_enb = '1';
    }
  }
}
FHM_DL_FLAG_UPDATE
}

{
    print <<FHM_DL_STATUS_READ
/** read status register */
function status_read{
  input{
  } 

  output{
    bit [$w_1:0] status_out;
  }

  control{
  }

  protocol{
  }
}

FHM_DL_STATUS_READ
}

{
    print <<FHM_DL_STATUS_WRITE
/** write status register **/
function status_write{
